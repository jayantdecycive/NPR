@model cfares.domain._event.Occurrence        
@{string pk = Model.IsBound() ? Model.Id().ToString() : "new";}  
    
    <fieldset data-pk='@pk' class='occurrence-@pk'>
        
        <div class='legend inline-block'>Occurrence</div>

        <div class='model-field field-Start'>
            <div class="display-label inline-block">Start</div>
            <div class="display-field inline-block">
                @Html.DisplayFor(model => model.Start)
            </div>
        </div>

        <div class='model-field field-End'>
            <div class="display-label inline-block">End</div>
            <div class="display-field inline-block">
                @Html.DisplayFor(model => model.End)
            </div>
        </div>

        <div class='model-field field-Reservation'>
            <div class="display-label inline-block">Reservation Event</div>
            <div class="display-field inline-block">
                @Html.DisplayFor(model => model.ResEvent)
            </div>
        </div>

        <div class='model-field field-Slots'>
            <div class="display-label inline-block">Slots</div>
            <div class="display-field inline-block">
                @Html.DisplayFor(model => model.Slots)
            </div>
        </div>

        <div class='model-field field-Store'>
            <div class="display-label inline-block">Store</div>
            <div class="display-field inline-block">
                @Html.DisplayFor(model => model.Store)
            </div>
        </div>

        <div class='model-field field-RegistrationAvailability'>
            <div class="display-label inline-block">Registration Availability</div>
            <div class="display-field inline-block">
                @Html.DisplayFor(model => model.RegistrationAvailability)
            </div>
        </div>

           

    </fieldset>