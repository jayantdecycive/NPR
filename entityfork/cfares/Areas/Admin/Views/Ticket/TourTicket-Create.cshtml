    Layout = "~/Areas/Admin/Views/Shared/Layout/_Create.cshtml";
    ViewBag.Brief = "Tour Ticket Detailed Information";
}


<h1>Create Ticket</h1>
<p>
    Register Customer
    <br />
    <br />
    @*Info <strong>info info</strong>*@

    
</p>
<div class='admin-green add-user'>
