@model cfares.Areas.tours.Models.StoryTourTicketFormModel

<div class="review-section" id="additional-stops">
            <span>Are there any additional tour stops you would like to take your team on? <span
                class="legal-aside">(click all that apply)</span></span>
            <div class=" name-value-pair ">
                <div class="checkboxgroup">
                    <span class="value checkbox-name-wrapper">@Html.CheckBoxFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitMarketing, new { @readonly = "readonly", disabled = "true" })
                        @Html.LabelFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitMarketing)</span>
                    <span class="value checkbox-name-wrapper">@Html.CheckBoxFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitTech, new { @readonly = "readonly", disabled = "true" })
                        @Html.LabelFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitTech)</span>
                    <span class="value checkbox-name-wrapper">@Html.CheckBoxFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitInnovation, new { @readonly = "readonly", disabled = "true" })
                        @Html.LabelFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitInnovation)
                    </span><span class="value checkbox-name-wrapper">@Html.CheckBoxFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitTraining, new { @readonly = "readonly", disabled = "true" })
                        @Html.LabelFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitTraining)
                    </span><span class="value checkbox-name-wrapper">@Html.CheckBoxFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitWellness, new { @readonly = "readonly", disabled = "true" })
                        @Html.LabelFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitWellness)
                    </span><span class="value checkbox-name-wrapper">@Html.CheckBoxFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitWareHouse, new { @readonly = "readonly", disabled = "true" })
                        @Html.LabelFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitWareHouse)
                    </span><span class="checkboxWithExplanation value checkbox-name-wrapper">@Html.CheckBoxFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitOther, new { @readonly = "readonly", disabled = "true" })
                        @Html.LabelFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitOther)
                        @Html.TextBoxFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitOtherDescription, new { @readonly = "readonly", disabled = "true" })</span>
                    @Html.ValidationMessageFor(m => ((cfares.Areas.tours.Models.StoryTourTicketFormModel)m).tourTicket.VisitOtherDescription)
                </div>
            </div>
        </div>