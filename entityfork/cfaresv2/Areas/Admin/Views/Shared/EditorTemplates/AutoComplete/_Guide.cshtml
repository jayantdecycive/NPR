@model cfacore.domain.user.User

@{ 
    string defaultLabel;
    if(Model==null || string.IsNullOrEmpty(Model.Id())){
        defaultLabel = "No User";
    }else if (string.IsNullOrEmpty(Model.Name.ToString())) {
        defaultLabel = string.Format("User id:{0}", Model.Id());
    }
    else {
        defaultLabel = Model.Name.ToString();
    }
    string id = string.Empty;
    if(Model!=null){
        id = Model.Id();
    }
    if(ViewData["DefaultLabel"]!=null){
        defaultLabel = ViewData["DefaultLabel"].ToString();
    }
}
<span class='admin-grey'>    
    <input type="text" data-default-label="@defaultLabel" value="@id" name="@ViewData.TemplateInfo.HtmlFieldPrefix" class='jq-auto-complete text-box' data-pk="UserId" 
        data-columns="Email,Name,UserId,Username" data-filter="Email,Name,Username,UserIdL" data-numeric-filter="UserId" data-label="Name" data-src='/DataService/User.svc/User_Guide' />
</span>

