@model cfares.domain._event._ticket.tours.TourTicket

@{    
    ViewBag.Title = "Edit";
    ViewBag.Brief = "Edit Ticket";
    Layout = "~/Areas/Admin/Views/Shared/Layout/_Edit.cshtml";
}


<h1>Edit Tour Ticket</h1>
<p>
    Enter ticket information.
    <br />
    @*Info <strong>info info</strong>*@
</p>

@using (Html.BeginForm())
{
@Html.ValidationSummary(true)
@Html.Partial("ModelForm/Tickets/_TourTicket")
@Html.Partial("ModelForm/_Ticket")
    <div class='submit-block admin-green'>
        <div class='editor-label inline-block inline-block inline-block'>
            
        </div>
        <div class='editor-field inline-block inline-block inline-block'>
            <input type="submit" data-icon="ui-icon-disk" class='jq-button' value="Save" />
        </div>
            
    </div>
}
@section Action{
    <div class='button-cta' data-icon="ui-icon-pencil">
        @Html.ActionLink("Delete", "TourTicket-Delete", new { id = Model.Id() })
    </div>
}
@section Cancel{
    <div class='dash-cta'>
        @Html.ActionLink("Cancel", "TourTicket-Details", new { id = Model.Id() })
    </div>
}