@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Chick-fil-A Customer Reception";
    ViewBag.ShowFaqs = false;
}

@*----------------------------------------------------
	Family Influence Template / Layout Overrides
----------------------------------------------------*@



@this.RedefineSection("Head")

@section head{
    <meta name="description" content="Participating Chick-fil-A restaurants are hosting a customer reception event with speaker, Dan Cathy.">
    <link rel="canonical" href="http://www.cfareception.com" />
}


@section Header{
    
    <a href="/Index"><img class="hidden-phone" id="theme_mast" src="@Html.ThemeContent("~/images/desktop/mast.png")" /></a>

    <hr class="hidden-phone" />
}

@this.RedefineSection("Banner")
@*@this.RedefineSection("Banner", 
	@<div class="banner">Example Sublayout Override for Banner</div>)*@
<div class="row-fluid">
    <section id="masthead">
        
    </section>
	
</div>

@RenderBody()
@this.RedefineSection("SocialFooter")
@this.RedefineSection("Footer")
@this.RedefineSection("Scripts")
